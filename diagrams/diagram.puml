@startuml

!define C4Container https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4_Container.puml
!include C4Container

skinparam svgLinkTarget foo

System_Boundary(c1, "edx-platform: edx-openuchile", $link="https://github.com/open-uchile/edx-openuchile") {
    System_Boundary(c2, "requirements", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/") {
        
            System_Boundary(apis, "apis", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/apis.txt") {
            
                Container(apis1, "portal_api", $link="https://github.com/open-uchile/portal_api")
            
            }
        
            System_Boundary(apps, "apps", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/apps.txt") {
            
                Container(apps1, "eol_auto_login", $link="https://github.com/eol-uchile/eol_auto_login")
            
                Container(apps2, "eol_contact_form", $link="https://github.com/open-uchile/eol_contact_form")
            
                Container(apps3, "course_classification", $link="https://github.com/eol-uchile/course_classification")
            
                Container(apps4, "edx-proctoring", $link="https://github.com/open-uchile/edx-proctoring")
            
                Container(apps5, "edx-search", $link="https://github.com/eol-uchile/edx-search")
            
                Container(apps6, "uchileedxlogin", $link="https://github.com/open-uchile/uchileedxlogin")
            
                Container(apps7, "eol_vimeo", $link="https://github.com/open-uchile/eol_vimeo")
            
            }
        
            System_Boundary(python_packages, "python_packages", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/python_packages.txt") {
            
                Container(python_packages1, "django-redis")
            
                Container(python_packages2, "fluent-logger")
            
                Container(python_packages3, "python-json-logger")
            
            }
        
            System_Boundary(reports, "reports", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/reports.txt") {
            
                Container(reports1, "eol_report_analytics", $link="https://github.com/eol-uchile/eol_report_analytics")
            
            }
        
            System_Boundary(tabs_plugins, "tabs_plugins", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/tabs_plugins.txt") {
            
                Container(tabs_plugins1, "eol_completion", $link="https://github.com/eol-uchile/eol_completion")
            
                Container(tabs_plugins2, "eol_welcome_mail", $link="https://github.com/eol-uchile/eol_welcome_mail")
            
            }
        
            System_Boundary(xblocks, "xblocks", $link="https://github.com/open-uchile/edx-openuchile/blob/main/requirements/xblocks.txt") {
            
                Container(xblocks1, "eolcontainer-xblock", $link="https://github.com/eol-uchile/eol-container-xblock.git")
            
                Container(xblocks2, "corfo_generate_code", $link="https://github.com/eol-uchile/corfo_generate_code")
            
                Container(xblocks3, "eolcourseprogram-xblock", $link="https://github.com/eol-uchile/eol-course-program-xblock")
            
                Container(xblocks4, "eoldiscussion", $link="https://github.com/eol-uchile/eol_xblock_discussion")
            
                Container(xblocks5, "eol_forum_notifications", $link="https://github.com/eol-uchile/eol_forum_notifications")
            
                Container(xblocks6, "eol_gradeforum_xblock", $link="https://github.com/eol-uchile/eol_gradeforum_xblock")
            
                Container(xblocks7, "eol_list_grade", $link="https://github.com/eol-uchile/eol_list_grade")
            
                Container(xblocks8, "ora2", $link="https://github.com/eol-uchile/edx-ora2.git")
            
                Container(xblocks9, "pdfXBlock", $link="https://github.com/open-uchile/pdfXBlock")
            
                Container(xblocks10, "scormxblock", $link="https://github.com/eol-uchile/edx_xblock_scorm")
            
                Container(xblocks11, "edx-sga", $link="https://github.com/eol-uchile/edx-sga")
            
                Container(xblocks12, "ubcpi", $link="https://github.com/eol-uchile/ubcpi")
            
                Container(xblocks13, "eolzoom-xblock", $link="https://github.com/eol-uchile/eol-zoom-xblock")
            
            }
        
    }

    System_Boundary(theme, "Themes", $link="https://github.com/open-uchile/edx-openuchile/blob/main/themes/") {
        
            Container(theme1, "open-uchile-theme", $link="https://github.com/open-uchile/open-uchile-theme")
        
    }

    System_Boundary(dockerfile, "edx-platform base", $link="https://github.com/open-uchile/edx-openuchile/blob/main/Dockerfile") {
            Container(dk, "9c53127fec822ddab991d8780d030ff9b4dfe4ce", $link="https://ghcr.io/open-uchile/edx-platform:9c53127fec822ddab991d8780d030ff9b4dfe4ce")
    }
}

SHOW_LEGEND()

@enduml